{
  "config": {
    "IS_THEME_TRANSITIONS_ENABLED": {
      "$value": true,
      "$type": "boolean",
      "$unit": "",
      "$meta": {
        "descritpion": "Enable transitions while theme changing",
        "category": "config"
      }
    },
    "IS_FULL_RUNTIME": {
      "$value": true,
      "$type": "boolean",
      "$unit": "",
      "$meta": {
        "descritpion": "Switch between hybrid and full runtime modes",
        "category": "config"
      }
    },
    "SCALE_FACTOR": {
      "$value": 1,
      "$type": "number",
      "$unit": "",
      "$meta": {
        "description": "Global font scale factor to unify font sizes across platforms. Implementation for mobile platforms is not active yet",
        "category": "config",
        "build": {
          "web": {
            "exportAsVar": true,
            "varName": "--scale-factor"
          },
          "ios": {
            "applyMultiplier": true,
            "status": "unused"
          },
          "android": {
            "applyMultiplier": true,
            "status": "unused"
          }
        }
      }
    },
    "DENSITY_SPACE": {
      "$value": 1,
      "$type": "number",
      "meta": {
        "build": {
          "web": {
            "exportAsVar": true,
            "varName": "--density-space"
          }
        }
      }
    },
    "DENSITY_LINE": {
      "$value": 1.35,
      "$type": "number",
      "meta": {
        "build": {
          "web": {
            "exportAsVar": true,
            "varName": "--density-line"
          }
        }
      }
    },
    "DENSITY_TARGET": {
      "$value": 40,
      "$type": "number",
      "meta": {
        "build": {
          "web": {
            "exportAsVar": true,
            "varName": "--density-target"
          }
        }
      }
    }
  }
}