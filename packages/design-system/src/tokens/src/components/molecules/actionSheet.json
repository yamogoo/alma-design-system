{
  "default": {
    "lg": {
      "root": {
        "width": {
          "$value": 860,
          "$type": "number",
          "$unit": "px",
          "$respond": {
            "below": {
              "md": {
                "$value": 100,
                "$type": "number",
                "$unit": "%"
              }
            }
          }
        },
        "height": {
          "$value": 540,
          "$type": "number",
          "$unit": "px",
          "$respond": {
            "below": {
              "md": {
                "$value": 100,
                "$type": "number",
                "$unit": "%"
              }
            }
          }
        },
        "borderRadius": {
          "$value": "{tokens.roundness.xxl}",
          "$type": "number",
          "$unit": "px",
          "$respond": {
            "below": {
              "md": {
                "$value": 0,
                "$type": "number",
                "$unit": "px"
              }
            }
          }
        },
        "padding": {
          "$value": [
            "{tokens.spacing.xxl}",
            "{tokens.spacing.lg}",
            "{tokens.spacing.lg}",
            "{tokens.spacing.lg}"
          ],
          "$type": "number",
          "$unit": "px"
        }
      },
      "surface": {
        "variant": {
          "$value": "default",
          "$type": "string",
          "$unit": "alias"
        },
        "size": {
          "$value": "md",
          "$type": "string",
          "$unit": "alias"
        }
      }
    }
  }
