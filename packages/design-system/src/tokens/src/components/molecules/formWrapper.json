{
  "default": {
    "lg": {
      "root": {
        "padding": {
          "$value": [
            "{tokens.spacing.md.$value}",
            "{tokens.spacing.lg.$value}"
          ],
          "$$type": "number",
          "$unit": "px"
        },
        "borderRadius": {
          "$value": "{tokens.roundness.xxl.$value}",
          "$$type": "number",
          "$unit": "px"
        },
        "borderWidth": {
          "$value": "{tokens.outline.$value}",
          "$$type": "number",
          "$unit": "px"
        }
      },
      "header": {
        "padding": {
          "$value": ["{tokens.spacing.sm.$value}", 0],
          "$$type": "number",
          "$unit": "px"
        }
      }
    }
  }
}
