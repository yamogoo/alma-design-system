{
  "namespace": {
    "$value": "al-",
    "$type": "string",
    "$unit": "alias",
    "$meta": {
      "description": "Global component prefix",
      "category": "config",
      "status": "unused"
    }
  },
  "mode": {
    "$value": "runtime",
    "$type": "string",
    "$unit": "alias",
    "$meta": {
      "description": "runtime | precompile | hybrid modes for components",
      "category": "config",
      "status": "deprecated"
    }
  },
  "motion": {
    "mode": {
      "$value": "{config.mode}",
      "$type": "string",
      "$unit": "alias",
      "$meta": {
        "description": "runtime | precompile | hybrid",
        "category": "config",
        "status": "unused"
      }
    },
    "enableTheme": {
      "$value": true,
      "$type": "boolean",
      "$unit": "",
      "$meta": {
        "description": "Enable transitions while theme changing",
        "category": "config",
        "status": "unused"
      }
    }
  },
  "scaleFactor": {
    "$value": 1,
    "$type": "number",
    "$unit": "",
    "$meta": {
      "description": "Global font scale factor to unify font sizes across platforms. Implementation for mobile platforms is not active yet",
      "category": "config",
      "status": "deprecated",
      "build": {
        "web": {
          "exportAsVar": true,
          "varName": "--scale-factor"
        },
        "ios": {
          "applyMultiplier": true,
          "status": "unused"
        },
        "android": {
          "applyMultiplier": true,
          "status": "unused"
        }
      }
    }
  },
  "densitySpace": {
    "$value": 1,
    "$type": "number",
    "meta": {
      "status": "deprecated",
      "build": { "web": { "exportAsVar": true, "varName": "--density-space" } }
    }
  },
  "densityLine": {
    "$value": 1.35,
    "$type": "number",
    "meta": {
      "status": "deprecated",
      "build": { "web": { "exportAsVar": true, "varName": "--density-line" } }
    }
  },
  "densityTarget": {
    "$value": 40,
    "$type": "number",
    "meta": {
      "status": "deprecated",
      "build": { "web": { "exportAsVar": true, "varName": "--density-target" } }
    }
  }
}
