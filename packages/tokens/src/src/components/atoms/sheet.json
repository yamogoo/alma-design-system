          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Border radius for large action sheet",
            "category": "sheet"
          }
        },
        "gap": {
          "$value": "{tokens.spacing.md.$value}",
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Gap between elements inside medium sheet",
            "category": "sheet"
          }
        },
        "padding": {
          "$value": [
            "{tokens.spacing.lg.$value}",
            "{tokens.spacing.lg.$value}"
          ],
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Padding inside medium sheet",
            "category": "sheet"
          }
        }
      },
      "button": {
        "offset": {
          "$value": "{tokens.spacing.lg.$value}",
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Offset for buttons inside medium sheet",
            "category": "sheet"
          }
        }
      }
    }
  }
