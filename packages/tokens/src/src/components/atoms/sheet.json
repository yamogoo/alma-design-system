{
  "default": {
    "lg": {
      "root": {
        "min-height": {
          "$value": 32,
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Minimum height of large action sheet",
            "category": "sheet"
          }
        },
        "borderRadius": {
          "$value": "{tokens.roundness.md.$value}",
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Border radius for large action sheet",
            "category": "sheet"
          }
        },
        "gap": {
          "$value": "{tokens.spacing.xl.$value}",
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Gap between elements inside large sheet",
            "category": "sheet"
          }
        },
        "padding": {
          "$value": [
            "{tokens.spacing.xxl.$value}",
            "{tokens.spacing.lg.$value}",
            "{tokens.spacing.lg.$value}",
            "{tokens.spacing.lg.$value}"
          ],
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Padding inside large sheet",
            "category": "sheet"
          }
        }
      },
      "button": {
        "offset": {
          "$value": "{tokens.spacing.lg.$value}",
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Offset for buttons inside large sheet",
            "category": "sheet"
          }
        }
      }
    },
    "md": {
      "root": {
        "min-height": {
          "$value": 32,
          "$type": "number",
          "$unit": "px",
          "$meta": {
            "description": "Minimum height of large action sheet",
